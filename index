#include <stdio.h>
#include <stdlib.h>
#define SIZE 5
int main(void)
{
	int i;
	int grade[SIZE]; // 타입 배열명[원소 개수] => 배열선언

	printf("5명의 점수를 입력하시오.\n");

	for(i=0;i<SIZE;i++)
		scanf("%d", &grade[i]); // 배열명[인덱스] => 배열원소 = 일반변수
	for(i=0;i<SIZE;i++)
		printf("grade[%d] = %d\n", i, grade[i]);
	return 0;
}
